{
  "expo": {
    "name": "TransBox",
    "slug": "TransBox",
    "privacy": "public",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcfcfc"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "VIBRATE"
      ],
      "package": "com.cybil.TransBox",
      "googleServicesFile": "./google-services.json"
    }
  }
}
