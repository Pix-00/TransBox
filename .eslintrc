{
    "extends": "@react-native-community",
    "rules": {
        "prettier/prettier": "off",
        "react-native/no-inline-styles": "off",
        "react/jsx-no-undef": "off",
        "@typescript-eslint/no-unused-vars": "off",
        "jsx-quotes": [
            "warn",
            "prefer-single"
        ]
    }
}
